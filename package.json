{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf coverage && rm -rf reports && rm -rf out/*",
    "lint": "eslint . --ext .js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --runInBand",
    "lint:ci": "yarn lint --quiet --format junit -o reports/junit/js-lint-results.xml",
    "build": "yarn clean && webpack --bail",
    "build:watch": "yarn clean && webpack --watch --progress",
    "build:fragmentTypes": "cd maharo && node fragment-types-builder.js",
    "start": "yarn clean && webpack-dev-server --config webpack.hot.config.js --progress",
    "clean:ssr": "rm -rf build",
    "build:ssr": "webpack --config webpack.ssr.config.js",
    "watch:ssr": "yarn build:ssr --watch",
    "wait:ssr": "mkdir build && cp wait.js build/server.js",
    "nodemon:ssr": "nodemon bin/ssr.js --watch ./build | bunyan",
    "ssr:dev": "yarn clean:ssr && yarn wait:ssr && npm-run-all --parallel watch:ssr nodemon:ssr",
    "ssr:prod": "node bin/ssr.js",
    "docker:build": "docker build -t ssr-local .",
    "docker:run": "docker run --env GRAPHQL_URL=http://docker.for.mac.localhost:5000/graphql -p 5001:5001 ssr-local",
    "upload-bundles": "node bin/upload-bundles.js out/manifest.json",
    "release-bundles": "node bin/release-bundles.js out/manifest.json"
  },
  "dependencies": {
    "@babel/core": "7.11.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.10.4",
    "@babel/plugin-proposal-numeric-separator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.11.1",
    "@google-cloud/storage": "^5.1.2",
    "@hot-loader/react-dom": "16.13.0",
    "@loadable/component": "^5.13.1",
    "@sentry/browser": "^5.15.4",
    "@sentry/node": "^5.15.5",
 
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-context": "1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "bunyan": "^1.8.12",
    "clipboard-polyfill": "2.7.0",
    "compression-webpack-plugin": "^6.0.1",
    "css-loader": "4.3.0",
    "d3-color": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-shape": "^2.0.0",
    "date-fns": "^2.16.1",
    "debug": "4.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "enzyme-to-json": "3.5.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^2.2.0",
    "glamor": "2.20.40",
    "glamorous": "5.0.0",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    
    "jest-glamor-react": "4.3.1",
    "jest-junit": "11.1.0",
    "js-yaml": "^3.14.0",
    "md5-o-matic": "0.1.1",
    "mobx": "3.5.1",
    "mobx-react": "4.4.3",
    "node-fetch": "^2.6.1",
    "node-noop": "1.0.0",
    
  },
  "engines": {
    "node": "^14.7.0",
    "yarn": "^1.22.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "snapshotSerializers": [
      "jest-glamor-react",
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".+\\.(svg|css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "<rootDir>/tests/empty-module.js"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3"
  },
  "resolutions": {
    "babel-plugin-inline-react-svg/svgo/js-yaml": "^3.14.0",
    "draft-js/fbjs/cross-fetch/node-fetch": "^2.6.1",
    "glamor/fbjs/isomorphic-fetch/node-fetch": "^2.6.1",
    "http-server/ecstatic": "4.1.4"
  }
}
